\chapter{扩展立方体的结构连通度和子结构连通度}
\label{Chap03}

连通度是衡量互连网络稳定性的一个重要指标。相对于传统连通度只将顶点发生故障当做单一事件相比，互连网络中的结构发生故障更能反映真实运行环境中网络的故障情况。在这一章中，我们研究了扩展立方体的$H$-结构连通度和$H$-子结构连通度，其中$H \in \{K_{1,M},P_L,C_N\}$，
且$1 \leq M \leq 6, 1 \leq L \leq 2n-1, 3 \leq N \leq 2n-1$。
同时我们给出了在扩展立方体中寻找两个无故障顶点的容错路由算法AQFRouting，当网络中的故障顶点数量在一定范围之内，该算法可以实现两个无故障顶点之间的正常通信。

\section{预备知识}

在给出扩展立方体结构连通度和子结构连通度的证明之前，我们先给出在本节需要用到的扩展立方体的一些定义和性质：

设$u$是$AQ_n$中的任意一个顶点。为了使我们的证明更加的简便，
我们引入一些符号$v^{[1]},v^{[2]},\ldots,v^{[2n-1]}$，
其中$ v^{[1]},v^{[2]},\ldots,v^{[2n-1]}$和$u$的所有邻居顶点
$u^1,u^2,\bar{u}^{2},\ldots,u^{n},\bar{u}^{n}$形成一一对应的关系。
$u^j$和 $v^{[i]}$ 的对应关系为：
（1）如果$i$是偶数，则$j=\frac{i}{2}+1$且$u^j=v^{[i]}$；
（2）如果$i$是奇数，则$j=\lceil\frac{i}{2}\rceil$且$\bar{u}^j=v^{[i]}$。
$(v^{[i]})^l$ 和$(\overline{v^{[i]}})^l$的定义类似于$u^l$和$\bar{u}^l$的定义。
例如，如果$u=000000$是$AQ_6$的一个顶点，那么$u^4=v^{[6]}=001000$，
$\bar{u}^5=v^{[9]}=011111$，$(v^{[6]})^2=001010$和$(\overline{v^{[9]}})^3=011000$。
在本章中，我们对这两种表示方法不加以区分。

\begin{thm}
\upcite{Choudum2002}
\label{k(AQ123)}
$\kappa(AQ_1) = 1, \kappa(AQ_2) = 3, \kappa(AQ_3) = 4$， 对于  $ n \geq 4,\kappa(AQ_n) = 2n-1$。
\end{thm}

根据定理\ref{k(AQ123)}，我们可以得到下面的定理。

\begin{thm}
对于$n \geq 4$，有$\kappa(AQ_n,K_1)=\kappa^s(AQ_n,K_1)=2n-1$。
\end{thm}

下面我们给出本章要用到的一些定理和引理。

\begin{thm}
\upcite{Ma2008}
\label{1_extraconnectivity}
对于$n \geq 6$，有$\kappa_1(AQ_n)= 4n - 8$。
\end{thm}


%下面几个应该是性质，而不是引理
\begin{lem}
\upcite{Ma2008}
\label{hypercube_edge}
如果$(u,u^i)$是$AQ_n$的一条$i$-维的超立方体边$(1 \leq i \leq n)$，那么
\[
{N_{AQ_n}(u)}\cap {N_{AQ_n}(u^i)}=\left\{
             \begin{array}{lcr}
             \{\bar{u}^i,\bar{u}^{i-1}\} & &2\leq i\leq n,\\
             \{\bar{u}^2,u^2\} & &i=1.
             \end{array}
\right.
\]

也就是说，$u$和$u^i$在$AQ_n$中有$2$个共同的邻居顶点并且$|{N_{AQ_n}}(\{u,u^i\})|= 4n-6$。
\end{lem}

\begin{lem}
\upcite{Ma2008}
\label{complement_edge}
如果$(u,\bar{u}^i)$是$AQ_n$的一条$i$-维的补边$(2 \leq i \leq n)$，那么
\[
{N_{AQ_n}(u)}\cap {N_{AQ_n}(\bar{u}^i)}=\left\{
             \begin{array}{lcr}
             \{\bar{u}^{i-1},u^i,u^{i+1},\bar{u}^{i+1}\} & &2\leq i\leq n-1,\\
             \{\bar{u}^{n-1},u^n\} & &i=n.
             \end{array}
\right.
\]

也就是说，$u$和$\bar{u}^i$在$AQ_n$中有$4$个共同的邻居顶点，对于$2 \leq i \leq n-1$，有$|{N_{AQ_n}}(\{u,\bar{u}^i\})|= 4n-8$。类似地，$u$和$\bar{u}^n$在$AQ_n$中有$2$ 个共同的邻居顶点，且$|{N_{AQ_n}}(\{u,\bar{u}^n\})|= 4n-6$。
\end{lem}

\begin{lem}
\upcite{Ma2008}
\label{common_verticles}
对于$n \geq 3$， $AQ_n$中的任意两个顶点最多有$4$个共同的邻居顶点。
\end{lem}

根据定义\ref{AugmentedCube1}，我们可以很容易地得到下面的关于扩展立方体的性质：

\begin{lem}
\label{two_hypercube_edges}
如果$(u,u^i)$和$(u,u^j)$是$AQ_n$的两条$i$-维和$j$-维的超立方体边$(1 \leq i \neq j \leq n)$。 不失一般性，设$i < j$，有
\[
{N_{AQ_n}(u^i)}\cap {N_{AQ_n}(u^j)}=\left\{
             \begin{array}{lcr}
             \{u,(u^i)^j,\bar{u}^i,({\bar{u}^j})^i\}  & & j=i+1 \quad\textnormal{且 }\quad i > 1,\\
             \{u,(u^i)^j\}  & & j>i+1,\\
             \{u,(u^1)^2\}  & & i=1\quad\textnormal{ 且 }\quad j=2.
             \end{array}
\right.
\]
\end{lem}

\begin{lem}
\label{two_complement_edges}
如果$(u,\bar{u}^i)$和$(u,\bar{u}^j)$是$AQ_n$的两条$i$-维和$j$-维的补边$(1 \leq i \neq j \leq n)$。 不失一般性，设$i < j$，有
\[
{N_{AQ_n}(\bar{u}^i)}\cap {N_{AQ_n}(\bar{u}^j)}=\left\{
             \begin{array}{lcr}
             \{u,\bar{u}^{i+1},(u^j)^{j-1},(\bar{u}^j)^{j-1}\}  & & j=i+2  ,\\
             \{u,(\overline{\bar{u}^j})^i\}  & & j=i+1 \quad\textnormal{或}\quad j>i+2.
             \end{array}
\right.
\]
\end{lem}

\begin{lem}
\label{hypercube_edge_and_complement_edge}
如果$(u,u^i)$是$AQ_n$的一条$i$-维的超立方体边，$(u,\bar{u}^j)$ 是$AQ_n$的一条$j$-维的补边$(1 \leq i,j \leq n)$，那么
\[
{N_{AQ_n}(u^i)}\cap {N_{AQ_n}(\bar{u}^j)}=\left\{
             \begin{array}{lcr}
             \{u,(u^i)^{i-1},(\bar{u}^i)^{i-1},\bar{u}^{i-1}\}  & & i=j\quad\textnormal{ 且 }\quad i>1,\\

             \{u,\bar{u}^i,(\bar{u}^j)^{i+1},({u}^i)^{i+1}\}  & & i=j+1\quad\textnormal\quad { 且 }i<n,\\
             \{u,\bar{u}^i\}  & & i=n\quad\textnormal{ 且 }\quad j=n-1,\\
             \{u,(u^i)^{i-1},(\bar{u}^i)^{i-1},\bar{u}^{i-1}\}  & & \quad i=j+2,\\
             \{u,(\bar{u}^j)^i\}  & & |i-j|>2,\\
             \{u,(u^j)^i,(\bar{u}^j)^i,\bar{u}^i\}  & & j=i+1\quad\textnormal{ 且 }\quad i>1,\\
             \{u,(u)^2\}  & & i = 1\quad\textnormal{ 且 }\quad j = 2,\\
             \{u,(\bar{u}^j)^i\}  & & j=i+2\quad\textnormal{ and }\quad i>1,\\
             \{u,\bar{u}^2,(\bar{u}^3)^1,(u^1)^3\}  & & i = 1\quad\textnormal{ 且 }\quad j = 3.
             \end{array}
\right.
\]
\end{lem}


\section{扩展立方体的$K_{1,M}-$结构连通度和子结构连通度}

根据扩展立方体的定义、引理\ref{hypercube_edge}和引理\ref{complement_edge}，
如果$u$是$AQ_n$的任意一个顶点且$(u,\bar{u}^i)$是$AQ_n$的$i$-维$(2\leq i \leq n-1)$的补边，
那么$N_{AQ_n}(u)\cap N_{AQ_n}(\bar{u}^i)=\{\bar{u}^{i-1},u^i,u^{i+1},\bar{u}^{i+1}\}$。
$\{\bar{u}^i,\bar{u}^{i-1},u^i,u^{i+1}, \bar{u}^{i+1}\}$ $(2\leq i \leq n-1)$的导出子图
与$K_{1,4}$同构。如果$(u,\bar{u}^n)$是$AQ_n$的$n$-维的补边，
那么$N_{AQ_n}(u)\cap N_{AQ_n}(\bar{u}^n)=\{\bar{u}^{n-1},u^n\}$
且$\{\bar{u}^n,\bar{u}^{n-1},u^n\}$的导出子图与$K_{1,2}$同构。
%{\color{red}
类似地，
如果$(u,u^i)$是$AQ_n$的$i$-维$(1\leq i \leq n)$的超立方体边，
那么$N_{AQ_n}(u)\cap N_{AQ_n}(u^i)=\{\bar{u}^{i},\bar{u}^{i-1}\}(2\leq i \leq n)$
且$N_{AQ_n}(u)\cap N_{AQ_n}(u^1)=\{u^{2},\bar{u}^{2}\}$。显然$\{u^i,\bar{u}^i$,$\bar{u}^{i-1}\}(2\leq i \leq n)$
的导出子图与$K_{1,2}$同构。
%}
接下来，我们将讨论$\kappa(AQ_n,K_{1,M})$和$\kappa^s(AQ_n,K_{1,M})$，
其中$1 \leq M \leq 3$，$4 \leq M \leq 6$。

%\subsection{$1 \leq M \leq 3$}
%\label{1<=M<=3}
我们首先证明当$1 \leq M \leq 3$时，$AQ_n$的结构连通度$\kappa(AQ_n,K_{1,M})$和子结构连通度$\kappa^s(AQ_n,K_{1,M})$。

\begin{lem}
\label{1<=M<=3(1)}
当$n \geq 4$且$1 \leq M \leq 3$时，$\kappa(AQ_n,K_{1,M})$ $\leq$ $\lceil\frac{2n-1}{1+M}\rceil$，$\kappa^s(AQ_n,K_{1,M})$ $\leq$ $\lceil\frac{2n-1}{1+M}\rceil$。
\end{lem}

\begin{proof}
设$u$是$AQ_n$的任意一个顶点。接下来，我们将根据$M$和$n$的值来分为下面几种情况：

\textbf{情形1}. $M=1$。设
\begin{flushleft}
$S_1=\{
\{
v^{[(M+1)i+1]},
v^{[(M+1)i+2]}
\}
\mid 0 \leq i < \lfloor\frac{2n-1}{M+1}\rfloor\}$，\\
$S_2=\{\{
v^{[2n-1]},
(v^{[2n-1]})^1
\}\}$。

\end{flushleft}

\textbf{情形2.} $M=2$。

%\textbf{情形2.1.} $n\equiv0$(模3)。 设
\textbf{情形2.1.} $n($模$3)=0$。 设
\begin{flushleft}
$S_1=\{\{
v^{[(M+1)i+1]},
v^{[(M+1)i+2]},
v^{[(M+1)i+3]}
\}
\mid 0 \leq i < \lfloor\frac{2n-1}{M+1}\rfloor\}$。

\end{flushleft}

%\textbf{情形2.2.} $n\equiv1$(模3)。设
\textbf{情形2.2.} $n($模$3)=1$。设
\begin{flushleft}
$S_1=\{\{
v^{[(M+1)i+1]},
v^{[(M+1)i+2]},
v^{[(M+1)i+3]}
\}
\mid 0 \leq i < \lfloor\frac{2n-1}{M+1}\rfloor\}$，\\
$S_2=\{\{
v^{[2n-1]},
(v^{[2n-1]})^1,
(v^{[2n-1]})^2
\}\}$。

\end{flushleft}

%\textbf{情形2.3.} $n\equiv2$(模3)。设
\textbf{情形2.3.} $n($模$3)=2$。设
\begin{flushleft}
$S_1=\{\{
v^{[(M+1)i+1]},
v^{[(M+1)i+2]},
v^{[(M+1)i+3]}
\}
\mid 0 \leq i < \lfloor\frac{2n-1}{M+1}\rfloor\}$，\\
$S_2=\{\{
v^{[2n-2]},
v^{[2n-1]},
(v^{[2n-1]})^1
\}\}$。

\end{flushleft}

\textbf{情形3.} $M=3$。

%\textbf{情形3.1.} $n\equiv1$(模4)。设
\textbf{情形3.1.} $n($模$4)=1$。设
\begin{flushleft}
$S_1=\{\{
v^{[(M+1)i+1]},
v^{[(M+1)i+2]},
v^{[(M+1)i+3]},
v^{[(M+1)i+4]}
\}
\mid 0 \leq i < \lfloor\frac{2n-1}{M+1}\rfloor\}$，\\
$S_2=\{\{
v^{[2n-1]},
(v^{[2n-1]})^1,
(v^{[2n-1]})^2,
(v^{[2n-1]})^3
\}\}$。

\end{flushleft}

%\textbf{情形3.2.} $n\equiv3$(模4)。设
\textbf{情形3.2.} $n($模$4)=3$。设
\begin{flushleft}
$S_1=\{\{
v^{[(M+1)i+1]},
v^{[(M+1)i+2]},
v^{[(M+1)i+3]},
v^{[(M+1)i+4]}
\}
\mid 0 \leq i < \lfloor\frac{2n-1}{M+1}\rfloor\}$，\\
$S_2=\{\{
v^{[2n-3]},
v^{[2n-2]},
v^{[2n-1]},
(v^{[2n-1]})^1
\}\}$。

\end{flushleft}

当$M = 2$且$n = 0($模$3)$时，设$S=S_1$，否则设$S=S_1 \cup S_2$。显然，如果$M=1$，那么$S_1 \cup S_2$中的每个元素的导出子图与$K_{1,1}$同构；如果$M=2$，当$0 \leq i < \lfloor\frac{2n-1}{M+1}\rfloor$时，顶点$v^{[(M+1)i+2]}$邻接于顶点$v^{[(M+1)i+1]}$和$v^{[(M+1)i+3]}$。此外，顶点$v^{[2n-1]}$邻接于顶点$v^{[2n-2]}$，$(v^{[2n-1]})^1$和$(v^{[2n-1]})^2$。因此，$S$中的每个元素的导出子图和$K_{1,2}$同构。如果$M=3$，当$0 \leq i < \lfloor\frac{2n-1}{M+1}\rfloor$时，顶点$v^{[(M+1)i+3]}$邻接于顶点$v^{[(M+1)i+1]}$，$v^{[(M+1)i+2]}$和$v^{[(M+1)i+4]}$。此外，顶点$v^{[2n-1]}$邻接于$v^{[2n-3]}$，$v^{[2n-2]}$，$(v^{[2n-1]})^1$，$(v^{[2n-1]})^2$和$(v^{[2n-1]})^3$。这样，$S$中每个元素的导出子图都与$K_{1,3}$同构。显然，$|S|$ = $\lceil\frac{2n-1}{1+M}\rceil$。设$Y(S)=\bigcup_{f \in S}f$，因为$AQ_n-Y(S)$是不连通的且其中一个连通分支是一个平凡图，所以$\kappa(AQ_n,K_{1,M})$ $\leq$ $\lceil\frac{2n-1}{1+M}\rceil$和$\kappa^s(AQ_n,K_{1,M})$ $\leq$ $\lceil\frac{2n-1}{1+M}\rceil$。
\end{proof}

